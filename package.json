{
  "name": "react-terminal-rendered-1",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build:demo": "babel examples --out-dir=demo --extensions '.ts,.tsx'",
    "build": "tsc --declaration --jsx react",
    "prebuild": "npm run clean",
    "clean": "rm -rf clean",
    "start": "npm run build && npm run build:demo && node demo/index.js",
    "check-types": "tsc --noEmit --jsx react",
    "lint": "eslint --ext ts,tsx,js,jsx src/",
    "fixlint": "eslint --fix --ext ts,tsx,js,jsx src/",
    "test": "npm run lint && npm run check-types"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "auto-bind": "^2.1.0",
    "log-update": "^3.2.0",
    "react": "^16.8.6",
    "react-reconciler": "^0.20.4",
    "readline": "^1.3.0",
    "scheduler": "^0.13.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.17",
    "@types/react-reconciler": "^0.18.0",
    "@types/scheduler": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "npm": "^6.9.0",
    "prettier": "1.16.4",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-react",
      "@babel/typescript"
    ]
  }
}
